<!-- This configuation file defines properties for a service hub -->

<wikipediaHubConfiguration>

    <!-- Options requred to allow retrieval of web content via proxy -->
    <proxy host="" port="" user="" password=""/>

    <!-- 
	A version of wikipedia that will be made available via the service hub. 
	You may specify multiple wikipedias, in which case one should be specified as the default
    -->
    <wikipedia name="" description="" default="true">
	path/to/conf/file
    </wikipedia>

</wikipediaHubConfiguration> 
